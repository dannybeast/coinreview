        // Charts for table
        $(document).ready( function () {
            
            // Datatables
            var table = $('.js-datatables').DataTable({
                "searching": false
            });

            // Styles table
            $('.js-datatables').each( function(){
                var $wrapper = $(this).parents('.table-wrapper');
                // filter
                var $filter = $wrapper.find('.table-wrapper__filter');
                $filter.append($wrapper.find('.dataTables_length'))
                $filter.append($wrapper.find('.dataTables_filter'))

                // pagination
                $wrapper.append('<div class="table-wrapper__footer"></div>')
                var $footer = $wrapper.find('.table-wrapper__footer');
                $footer.append($wrapper.find('.dataTables_info'))
                $footer.append($wrapper.find('.dataTables_paginate'))
            })

            table.on( 'draw.dt', function () {
                Charts.drawTableCharts()
            });
           

            let selectedType = $('[name="chart-type"]:checked').val();

            const detailsChart = LightweightCharts.createChart(document.querySelector('.chart-lightweight'), { 
                height: 450,
                timeScale: {
                    timeVisible: true,
                    secondsVisible: false,
                }, 
                
                });
            const series = detailsChart.addAreaSeries({	
                topColor: 'rgba(72, 124, 235, 0.4)',	
                bottomColor: 'rgba(0, 120, 255, 0.0)',
                lineColor: 'rgba(72, 124, 235, 1.0)',
                lineWidth: 2,
                priceFormat: {
                    precision: 5,
                },
            });
            

            const candleSeries = detailsChart.addCandlestickSeries();

            let areaData = []
            let candleData = []


            function setData(result){
                Object.values(result.data).forEach((el)=>{
                    areaData.push({
                        time: el.time,
                        value: +el.price
                    })

                    candleData.push({ 
                        time: el.time, 
                        open: +el.open, 
                        high: +el.high, 
                        low: +el.low, 
                        close: +el.close 
                    })
                    
                })

                console.log(areaData)

                console.log(candleData)

                if(selectedType === 'area'){
                    series.setData(areaData);
                }else if(selectedType === 'candle'){
                   candleSeries.setData(candleData);
                }
            }

